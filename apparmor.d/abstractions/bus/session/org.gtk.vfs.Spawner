  abi <abi/4.0>,

  dbus send bus=session path=/org/gtk/gvfs/exec_spaw/@{int}
       interface=org.gtk.vfs.Spawner
       member=Spawned
       peer=(name=@{busname}, label=gvfsd),

  include if exists <abstractions/bus/session/org.gtk.vfs.Spawner.d>

