  abi <abi/4.0>,

  dbus receive bus=session
       interface=org.gtk.Menus
       member={Start,End}
       peer=(name=@{busname}),

  dbus send bus=session
       interface=org.gtk.Menus
       member=Changed,

  include if exists <abstractions/bus/session/org.gtk.Menus.d>

# vim:syntax=apparmor
